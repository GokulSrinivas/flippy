{
  "manifest_version": 2,
  "name": "Flippy v0",
  "description": "Flippy",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "bookmarklet.html",
    "default_title": "Flipsearch"
  },
  "permissions": [
      "tabs",
      "http://*/*",
      "https://*/*"  
  ],
  "content_scripts": [
    {
      "matches": ["http://www.flipkart.com/*"],
      "js": ["bundle.js"]
    }
  ]
}